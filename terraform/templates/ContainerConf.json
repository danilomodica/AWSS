[
    {
        "name": "${name}",
        "image": "${repo}",
        "cpu": 0,
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${logGroup}",
                "awslogs-region": "eu-central-1",
                "awslogs-stream-prefix": "stream"
            }
        }
    }
]